  "entity.typemoonworld.ryougi_shiki.speech.crowded": "§7[Ryougi Shiki] Crowded...",
  "entity.typemoonworld.ryougi_shiki.speech.kill_creative": "§c[Ryougi Shiki] §4Everything that exists has a flaw.",
  "entity.typemoonworld.ryougi_shiki.speech.betrayed": "§c[Ryougi Shiki] ...I see.",
  "entity.typemoonworld.ryougi_shiki.speech.betrayed_ignore": "§c[Ryougi Shiki] (Ignores you)",
  "entity.typemoonworld.ryougi_shiki.speech.feed": "§7[Ryougi Shiki] ...Not bad.",
  "entity.typemoonworld.ryougi_shiki.speech.feed_high_trust": "§7[Ryougi Shiki] Hmph... Thanks.",
  "entity.typemoonworld.ryougi_shiki.speech.full_health": "§7[Ryougi Shiki] That's enough.",
  "entity.typemoonworld.ryougi_shiki.speech.void_fall": "§c[Ryougi Shiki] §4The concept of death... you cannot define it.",
  "entity.typemoonworld.ryougi_shiki.speech.see_sword": "§7[Ryougi Shiki] Even with a famous blade, it depends on the wielder.",
  "entity.typemoonworld.ryougi_shiki.speech.see_shield": "§7[Ryougi Shiki] There is no absolute defense for anything alive.",
  "entity.typemoonworld.ryougi_shiki.speech.see_clock": "§7[Ryougi Shiki] ...Waiting for someone?",
  "entity.typemoonworld.ryougi_shiki.speech.see_water": "§7[Ryougi Shiki] Water... I don't dislike it.",
  "entity.typemoonworld.ryougi_shiki.speech.see_milk": "§7[Ryougi Shiki] ...What do you think I am?",
  "entity.typemoonworld.ryougi_shiki.speech.see_map": "§7[Ryougi Shiki] Lost? Don't count on me.",
  "entity.typemoonworld.ryougi_shiki.speech.see_string": "§7[Ryougi Shiki] (Stares for a moment)... Boring.",
  "entity.typemoonworld.ryougi_shiki.speech.see_apple": "§7[Ryougi Shiki] An apple... reminds me of something.",
  "entity.typemoonworld.ryougi_shiki.speech.see_rabbit_foot": "§7[Ryougi Shiki] This kind of charm means nothing to me.",
  "entity.typemoonworld.ryougi_shiki.speech.see_compass": "§7[Ryougi Shiki] I decide my own direction.",
  "entity.typemoonworld.ryougi_shiki.speech.see_glass_bottle": "§7[Ryougi Shiki] Empty... just like something.",
  "entity.typemoonworld.ryougi_shiki.speech.see_flower": "§7[Ryougi Shiki] Flowers... will eventually wither.",
  "entity.typemoonworld.ryougi_shiki.speech.see_skull": "§7[Ryougi Shiki] Peace that only belongs to the dead.",
  "entity.typemoonworld.ryougi_shiki.speech.see_ice": "§7[Ryougi Shiki] ...Are you implying I'm cold?",
  "entity.typemoonworld.ryougi_shiki.speech.see_bed": "§7[Ryougi Shiki] Dreams... are also a reality.",
  "entity.typemoonworld.ryougi_shiki.speech.see_book": "§7[Ryougi Shiki] Books... I'm not interested in complex theories.",
  "entity.typemoonworld.ryougi_shiki.speech.see_shears": "§7[Ryougi Shiki] Cutting with this? ...Don't joke around.",
  "entity.typemoonworld.ryougi_shiki.speech.rain": "§7[Ryougi Shiki] Rain... even this weather can't wash away the smell of blood.",
  "entity.typemoonworld.ryougi_shiki.speech.moon": "§7[Ryougi Shiki] The moon tonight... is beautiful.",
  "entity.typemoonworld.ryougi_shiki.speech.cherry_blossom": "§7[Ryougi Shiki] Cherry blossoms... a scene fitting for parting.",
  "entity.typemoonworld.ryougi_shiki.speech.player_low_health": "§7[Ryougi Shiki] Hey, look at you... you're dying.",
  "entity.typemoonworld.ryougi_shiki.speech.player_invisible": "§7[Ryougi Shiki] Thought I couldn't see you? ...As long as you exist, there are lines.",
  "entity.typemoonworld.ryougi_shiki.speech.dodge": "§7[Ryougi Shiki] Too slow.",
  "entity.typemoonworld.ryougi_shiki.speech.guerrilla_retreat": "§7[Ryougi Shiki] Backing off... for now.",
  "entity.typemoonworld.ryougi_shiki.speech.ultimate_start": "§c[Ryougi Shiki] §4Chokushi (Mystic Eyes)—",
  "entity.typemoonworld.ryougi_shiki.speech.ultimate_execute": "§c[Ryougi Shiki] §4—I see it!",
  "entity.typemoonworld.ryougi_shiki.speech.help_kill": "§7[Ryougi Shiki] The obstacle has been removed.",
  "entity.typemoonworld.ryougi_shiki.speech.refused": "§c[Ryougi Shiki] §4...Refused."
